<h1><%= params[:controller].humanize %> - <%= params[:action].humanize %> - <%= params[:method].humanize %></h1>

<%= form_tag(update_test_relation_update_path(encode_method(params[:method]), params[:option])) do |f| %>
  <%= render @partial if @partial.present? %>
  <h2>Updates</h2>
  <%= fields_for "updates", AllTypesObject.new do |f| %>
    <%= render 'shared/fields', :f => f %>
  <% end %>
  <%= render 'shared/query_methods_fields' %>
  <div class="actions">
    <%= submit_tag("Update All types objects") %>
  </div>
<% end %>